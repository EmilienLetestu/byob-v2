<?xml version="1.0" encoding="UTF-8" ?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
        http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="App\Entity\Product">
        <id name="id" type="integer">
            <generator strategy="AUTO"/>
        </id>
        <field name="family" column="family" type="string" length="100"/>
        <field name="type" column="type" type="string" length="100"/>
        <field name="category" column="category" type="string" length="100"/>
        <field name="make" column="make" type="string" length="100"/>
        <field name="model" column="model" type="string" length="100"/>
        <field name="designation" column="designation" type="string" length="100"/>
        <field name="reference" column="reference" type="string" length="100" unique="true"/>
        <field name="referencedOn" column="referenced_on" type="date"/>
        <field name="lastModification" column="last_modification" type="date" nullable="true"/>
    </entity>
</doctrine-mapping>