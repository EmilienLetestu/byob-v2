<div class="col s12 m12">
    {{ form_start(form) }}
    <div>
        {{ form_label(form.product) }}
        {{ form_widget(form.product, {'attr': {'class' : 'browser-default'}}) }}
    </div>
    <div>
        {{ form_label(form.quantity) }}
        {{ form_widget(form.quantity, {'attr': {'step' : '10'}}) }}
    </div>
    <div>
        <button class="btn waves-effect waves-light color-action-darken" type="submit"><i class="material-icons right">send
            </i>Ajouter
        </button>
        <a href="#" class="btn waves-effect waves-light color-action">Finaliser</a>
    </div>
    {{ form_end(form) }}
</div>
